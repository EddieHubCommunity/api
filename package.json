{
  "name": "api",
  "version": "0.16.5",
  "description": "An API to manage our community data",
  "author": "EddieHubCommunity",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cucumber-js -p default --exit",
    "postinstall": "ncp ./.env.example ./.env"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.4",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.4",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/platform-socket.io": "^8.4.6",
    "@nestjs/swagger": "^5.0.4",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/websockets": "^8.4.6",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "helmet": "^4.6.0",
    "mongoose": "^6.0.12",
    "passport": "^0.4.1",
    "passport-unique-token": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@cucumber/cucumber": "^7.3.2",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.2",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.4.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "chai": "^4.3.4",
    "cucumber-tsflow": "^4.0.0-preview.7",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^12.3.7",
    "ncp": "^2.0.0",
    "prettier": "^2.6.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.1.0",
    "tsc-watch": "^4.4.0",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}